generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(cuid())
  email         String   @unique
  name          String?
  phone         String?
  youtubeHandle String?
  picked        Boolean  @default(false)
  pickedAt      DateTime?
  createdAt     DateTime @default(now())
}
